{
  "name": "bestiary-ui-monorepo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm -C playground dev",
    "dev:build": "pnpm -C playground-build dev",
    "pack:libs": "pnpm exec tsx scripts/pack.ts",
    "build": "tsx scripts/build.ts",
    "build:components": "tsx scripts/build.ts --components",
    "build:utils": "tsx scripts/build.ts --utils",
    "build:style": "tsx scripts/build.ts --style",
    "typecheck": "vue-tsc --noEmit -p tsconfig.base.json --composite false"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "esbuild": "^0.27.2",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  }
}