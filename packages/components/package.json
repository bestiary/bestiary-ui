{
    "name": "@bestiary-ui/components",
    "version": "0.0.1",
    "type": "module",
    "main": "../bestiary-ui/components/index.js",
    "module": "../bestiary-ui/components/index.js",
    "types": "../bestiary-ui/components/index.d.ts",
    "exports": {
        ".": {
            "types": "../bestiary-ui/components/index.d.ts",
            "import": "../bestiary-ui/components/index.js",
            "require": "../bestiary-ui/components/index.cjs"
        },
        "./button": {
            "types": "../bestiary-ui/components/general/button/index.d.ts",
            "import": "../bestiary-ui/components/general/button/index.js",
            "require": "../bestiary-ui/components/general/button/index.cjs"
        },

        "./badge": {
            "types": "../bestiary-ui/components/data-display/badge/index.d.ts",
            "import": "../bestiary-ui/components/data-display/badge/index.js",
            "require": "../bestiary-ui/components/data-display/badge/index.cjs"
        },
        "./overlaybadge": {
            "types": "../bestiary-ui/components/data-display/overlaybadge/index.d.ts",
            "import": "../bestiary-ui/components/data-display/overlaybadge/index.js",
            "require": "../bestiary-ui/components/data-display/overlaybadge/index.cjs"
        },
        "./card": {
            "types": "../bestiary-ui/components/data-display/card/index.d.ts",
            "import": "../bestiary-ui/components/data-display/card/index.js",
            "require": "../bestiary-ui/components/data-display/card/index.cjs"
        },

        "./divider": {
            "types": "../bestiary-ui/components/layout/divider/index.d.ts",
            "import": "../bestiary-ui/components/layout/divider/index.js",
            "require": "../bestiary-ui/components/layout/divider/index.cjs"
        },
        "./splitter": {
            "types": "../bestiary-ui/components/layout/splitter/index.d.ts",
            "import": "../bestiary-ui/components/layout/splitter/index.js",
            "require": "../bestiary-ui/components/layout/splitter/index.cjs"
        },
        "./splitterpanel": {
            "types": "../bestiary-ui/components/layout/splitterpanel/index.d.ts",
            "import": "../bestiary-ui/components/layout/splitterpanel/index.js",
            "require": "../bestiary-ui/components/layout/splitterpanel/index.cjs"
        },

        "./*": "./*"
    },
    "sideEffects": [
        "**/*.css"
    ],
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "vue": "^3.4.0",
        "@bestiary-ui/icons": "workspace:*",
        "@bestiary-ui/style": "workspace:*"
    },
    "peerDependenciesMeta": {
        "@bestiary-ui/icons": {
            "optional": true
        },
        "@bestiary-ui/style": {
            "optional": true
        }
    },
    "dependencies": {
        "@bestiary-ui/utils": "workspace:*"
    },
    "devDependencies": {
        "vite-plugin-lib-inject-css": "^2.2.2"
    }
}